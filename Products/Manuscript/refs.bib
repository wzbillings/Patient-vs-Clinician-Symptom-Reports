  file = {C\:\\Users\\Zane\\Zotero\\storage\\SQCCIJIQ\\Bland and Altman - 1995 - Comparing methods of measurement why plotting dif.pdf;C\:\\Users\\Zane\\Zotero\\storage\\2Y9SPBTL\\S0140673695917489.html}
}


}

@article{shrestha2011,
  title = {Estimating the Burden of 2009 Pandemic Influenza {{A}} ({{H1N1}}) in the {{United States}} ({{April}} 2009-{{April}} 2010)},
  author = {Shrestha, Sundar S. and Swerdlow, David L. and Borse, Rebekah H. and Prabhu, Vimalanand S. and Finelli, Lyn and Atkins, Charisma Y. and {Owusu-Edusei}, Kwame and Bell, Beth and Mead, Paul S. and Biggerstaff, Matthew and Brammer, Lynnette and Davidson, Heidi and Jernigan, Daniel and Jhung, Michael A. and Kamimoto, Laurie A. and Merlin, Toby L. and Nowell, Mackenzie and Redd, Stephen C. and Reed, Carrie and Schuchat, Anne and Meltzer, Martin I.},
  year = {2011},
  month = jan,
  journal = {Clin Infect Dis},
  volume = {52 Suppl 1},

  keywords = {Adult,Aged,{Aged, 80 and over},Attitude to Computers,electronic self-report,Female,Humans,integrative review,Internet,Male,Middle Aged,Oncology Nursing,patient self-report,patient-reported symptoms,Patients,Self Report,Symptom Assessment,Telemedicine}
}


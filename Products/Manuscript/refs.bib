@article{mccoul2019,
  title = {Differences in the {{Intended Meaning}} of {{Congestion Between Patients}} and {{Clinicians}}},
  author = {McCoul, Edward D. and Mohammed, Alaa E. and Debbaneh, Peter M. and Carratola, Maria and Patel, Amit S.},

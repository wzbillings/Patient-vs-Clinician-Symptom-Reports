@article{simonsen1998,
  title = {Pandemic versus Epidemic Influenza Mortality: A Pattern of Changing Age Distribution},
  shorttitle = {Pandemic versus Epidemic Influenza Mortality},
  author = {Simonsen, L. and Clarke, M. J. and Schonberger, L. B. and Arden, N. H. and Cox, N. J. and Fukuda, K.},
